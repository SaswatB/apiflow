{
  "comments": false,
  "env": {
    "test": {
      "presets": [
        ["@babel/preset-env", {
          "targets": { "electron": "4" }
        }]
      ],
      "plugins": ["istanbul"]
    },
    "main": {
      "presets": [
        ["@babel/preset-env", {
          "targets": { "electron": "4" }
        }]
      ]
    },
    "renderer": {
      "presets": [
        ["@babel/preset-env", {
          "targets": { "electron": "4" }
        }]
      ]
    },
    "web": {
      "presets": [
        ["@babel/preset-env", {
          "targets": { "chrome": "70" }
        }]
      ]
    }
  },
  "plugins": ["@babel/plugin-transform-runtime", "transform-vue-jsx"]
}
