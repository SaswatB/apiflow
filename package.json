{
  "name": "apiflow",
  "version": "0.0.1",
  "author": "Saswat Bhattacharya",
  "description": "An extensible api testing tool.",
  "license": "Apache-2.0",
  "main": "./dist/electron/main.js",
  "scripts": {
    "compile": "electron-webpack",
    "dev": "electron-webpack dev",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "e2e": "yarn run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue src test",
    "lint:fix": "eslint --ext .js,.vue --fix src test",
    "test": "yarn run unit && yarn run e2e",
    "unit": "karma start test/unit/karma.conf.js"
  },
  "build": {
    "productName": "apiflow",
    "appId": "com.hstar.apiflow",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "electronWebpack": {
    "main": {
      "webpackConfig": "webpack.additions.js"
    },
    "renderer": {
      "webpackConfig": "webpack.additions.js"
    }
  },
  "dependencies": {
    "@mdi/font": "^3.0.39",
    "@soleng-fuze/electron-debug": "^1.5.2",
    "axios": "^0.18.0",
    "content-type-parser": "^1.0.2",
    "d3": "^5.7.0",
    "d3-dag": "^0.1.2",
    "electron-promise-ipc": "^1.2.2",
    "element-ui": "^2.4.10",
    "js-beautify": "^1.8.8",
    "lodash": "^4.17.21",
    "node-libcurl": "^2.0.2",
    "parse-curl": "^0.2.6",
    "portal-vue": "^2.1.6",
    "ramjet": "^0.5.0",
    "recast": "^0.16.1",
    "source-map-support": "^0.5.12",
    "uuid": "^3.3.2",
    "v-tooltip": "^2.0.0-rc.33",
    "validator": "^10.9.0",
    "vm2": "^3.6.10",
    "vue": "^2.6.10",
    "vue-context": "^3.4.0",
    "vue-json-viewer": "^2.2.5",
    "vue-moment": "^4.0.0",
    "vue-overdrive": "^0.0.12",
    "vue-router": "^3.0.1",
    "vue-split-panel": "^1.0.4",
    "vue-tsx-support": "^2.3.0",
    "vue2-ace-editor": "^0.0.11",
    "vuex": "^3.0.1",
    "vuex-electron": "^1.0.0"
  },
  "devDependencies": {
    "@types/d3": "^5.7.2",
    "@types/electron-debug": "^1.1.0",
    "@types/js-beautify": "^1.8.1",
    "@types/lodash": "^4.14.134",
    "@types/node": "^12.0.7",
    "@types/uuid": "^3.4.4",
    "@types/validator": "^10.11.1",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/parser": "^1.13.0",
    "chai": "^4.1.2",
    "devtron": "^1.4.0",
    "electron": "^6.0.9",
    "electron-builder": "^21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-webpack": "^2.6.2",
    "electron-webpack-ts": "^3.2.0",
    "electron-webpack-vue": "^2.3.0",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^4.7.1",
    "ignore-not-found-export-webpack-plugin": "^1.0.0",
    "karma": "^3.1.1",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.2",
    "karma-electron": "^6.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^3.0.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.12.0",
    "require-dir": "^1.0.0",
    "sass-loader": "^7.0.3",
    "spectron": "^5.0.0",
    "typescript": "^3.5.1",
    "vue-class-component": "^6.3.2",
    "vue-property-decorator": "^7.2.0",
    "vue-template-compiler": "^2.6.10",
    "vuex-module-decorators": "^0.9.1",
    "webpack": "^4.33.0"
  }
}
